---
import Default from '@/layouts/Default.astro';
import {PAGES_METADATA} from '@/consts';
import NotFoundImage from '@/assets/404/404-page.png';

const {title, description} = PAGES_METADATA['404'];
---

<Default title={title} description={description} image={NotFoundImage}>
  <section>
    <h1>{title}</h1>
    <p>Page not found! :(</p>
    <p>
      Go to <a href='/'>start page</a>
    </p>
  </section>
</Default>
